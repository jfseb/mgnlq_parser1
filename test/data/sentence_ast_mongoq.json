    {
        "nr": 1,
        "query": "domains ending with ABC",
        "astNice": "BINOP -1(2)\n  OPAll -1(1)\n    LIST -1(1)\n      CAT 0\n  LIST -1(1)\n    OPEndsWith 1(2)\n      CAT 0\n      ANY 2\n",
        "match_json": [
            {
                "$match": "1"
            },
            {
                "domain": "2"
            },
            {
                "$regex": "3"
            },
            "__REGEXP /abc$/i"
        ],
        "group": {
            "$group": {
                "_id": {
                    "domain": "$domain"
                },
                "domain": {
                    "$first": "$domain"
                }
            }
        },
        "projection": {
            "$project": {
                "_id": 0,
                "domain": 1
            }
        }
    },
